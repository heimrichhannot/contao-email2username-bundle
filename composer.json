{
    "name": "heimrichhannot/contao-email2username-bundle",
    "description":"A Contao bundle that automatically sets the username based on the user email address.",
    "keywords":["contao"],
    "type": "contao-bundle",
    "homepage":"https://github.com/heimrichhannot/contao-email2username-bundle",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Heimrich & Hannot",
            "email":"digitales@heimrich-hannot.de",
            "homepage":"http://www.heimrich-hannot.de",
            "role":"Developer"
        }
    ],
    "support":{
        "issues":"https://github.com/heimrichhannot/contao-email2username-bundle/issues"
    },
    "require": {
        "php": "^7.1|^8.0",
        "contao/core-bundle": "^4.4",
        "heimrichhannot/contao-utils-bundle": "^2.181"
    },
    "replace": {
        "heimrichhannot/contao-email2username": "*"
    },
    "autoload": {
        "psr-4": {
            "HeimrichHannot\\Email2UsernameBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "HeimrichHannot\\Email2UsernameBundle\\ContaoManager\\Plugin"
    }
}
